      amount,
      category: {
        type
      }
    } of transactions) {
      if (type === "Income") {
        result += amount;
      }
    }

    return Object(_service_currencyService__WEBPACK_IMPORTED_MODULE_2__["formatThaiCurrency"])(result);
  }

  function calExpense(transactions) {
    let result = 0;

