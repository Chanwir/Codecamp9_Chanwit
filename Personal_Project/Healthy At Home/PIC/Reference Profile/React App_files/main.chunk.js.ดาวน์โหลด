function Summary(props) {
  const {
    transactions
  } = props;

  function calNetWorth(transactions) {
    let result = 0;

    for (let {
      amount,
      category: {
        type
      }
    } of transactions) {
      if (type === "Expense") {
        result -= amount;
      } else {
        result += amount;
      }
    }

    return Object(_service_currencyService__WEBPACK_IMPORTED_MODULE_2__["formatThaiCurrency"])(result);
  }

  function calIncome(transactions) {
    let result = 0;

